{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@versel/node",
            "config":{
                "includeFiles":["data/db.json"]
            }
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}
